{
    "name": "@syntaxflow/form",
    "version": "1.0.8",
    "description": "A comprehensive form library for React with Tailwind CSS v4",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./styles.css": "./dist/styles.css"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-hook-form": "^7.0.0"
    },
    "peerDependenciesMeta": {
        "react-router-dom": {
            "optional": true
        }
    },
    "dependencies": {
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "date-fns": "^4.1.0",
        "lucide-react": "^0.263.1",
        "react-phone-input-2": "^2.15.1",
        "tailwind-merge": "^2.2.0",
        "tailwindcss": "^4.1.17"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "keywords": [
        "react",
        "form",
        "react-hook-form",
        "tailwind",
        "tailwindcss",
        "form-components",
        "input",
        "validation"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/beloyosry/form-package"
    },
    "author": "Syntax Flow",
    "license": "MIT"
}
